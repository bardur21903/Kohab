if a % 3 ==0 and b % 3 ==0:
    print(True)
elif a % 3 ==0 or b % 3 ==0:
    print("–û–¥–Ω–æ —á–∏—Å–ª–æ –¥–µ–ª–∏—Ç—Å—è –Ω–∞ —Ç—Ä–∏")
