#11.1
prog_num=0
a=int(input())
b=int(input())
if a < 0 and b < 0:
    prog_num = a+b
    print(f"–û–±–∞ —á–∏—Å–ª–∞ –æ—Ç—Ä–∏—Ü–∞—Ç–µ–ª—å–Ω—ã—Ö. –°—É–º–º–∞: {prog_num}")
elif a >=0 and b >=0:
    prog_num = a-b
    print(f"–û–±–∞ —á–∏—Å–ª–∞ –ø–æ–ª–æ–∂–∏—Ç–µ–ª—å–Ω—ã—Ö –°—É–º–º–∞: {prog_num}")
else:
    print(False)

