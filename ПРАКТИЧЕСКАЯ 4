    print(a**3)
else:
    print(a*100)

#6.3
a= int(input())
b= int(input())
if a<0 :
    print(a+1000,b)
elif b<0:
    print(b+1000,a)
elif a<0 and b<0:
    print(False)
elif a>0 and b>0:
    print(True)
